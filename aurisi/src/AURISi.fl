    label {TM Parameter Details} open
    xywh {561 130 635 635} type Double visible
  } {
    Fl_Group groupFilter {
      label Filter open
      xywh {5 20 625 55} box THIN_UP_FRAME align 5
    } {}
    Fl_Group groupParamDetails {
      label Parameters open
      xywh {5 100 625 535} box THIN_DOWN_FRAME align 5
    } {}
  }
  code {let { paramDetailwindow = ParamDetailWindowFluid { 
     _pdwfWindow = tmParamDetailsWindow
